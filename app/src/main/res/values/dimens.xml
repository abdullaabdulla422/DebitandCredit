<?xml version="1.0" encoding="utf-8"?>
<resources>
    <dimen name="fab_margin">16dp</dimen>
    <dimen name="activity_margin">16dp</dimen>
    <dimen name="dot_margin_right">10dp</dimen>
    <dimen name="msg_no_notes">26sp</dimen>
    <dimen name="margin_top_no_notes">120dp</dimen>
    <dimen name="lbl_new_note_title">20sp</dimen>
    <dimen name="dimen_10">10dp</dimen>
    <dimen name="input_new_note">20sp</dimen>
    <dimen name="dot_height">30dp</dimen>
    <dimen name="dot_text_size">40sp</dimen>
    <dimen name="timestamp">14sp</dimen>
    <dimen name="note_list_text">18sp</dimen>

    <dimen name="standard_55">55dp</dimen>
    <dimen name="standard_105">105dp</dimen>
    <dimen name="standard_155">155dp</dimen>

    <declare-styleable name="FloatingActionButton">
        <!-- Background color -->
        <attr name="floatingActionButtonColor" format="color|reference"/>
        <!-- Size of action button -->
        <attr name="floatingActionButtonSize" format="enum">
            <!-- 56dp -->
            <enum name="normal" value="0"/>
            <!-- 40dp -->
            <enum name="mini" value="1"/>
        </attr>
        <!-- Elevation will be configured for API level 21 according to this attribute. Default value is true. -->
        <attr name="floatingActionButtonImplicitElevation" format="boolean"/>
        <!-- Show shadow or not, default value is true -->
        <attr name="floatingActionButtonShadow" format="boolean"/>
    </declare-styleable>
</resources>